{
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "schemes": [
        "http"
    ],
    "swagger": "2.0",
    "info": {
        "description": "Описание взаимодействия и работы внутренних моделей E2E CRM",
        "title": "E2E",
        "contact": {
            "name": "Repo",
            "url": "https://github.com/GeekchanskiY/E2E"
        },
        "license": {
            "name": "MIT License",
            "url": "https://github.com/GeekchanskiY/E2E/blob/main/LICENSE"
        },
        "version": "0.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/users/register": {
            "post": {
                "description": "Registers user and creates permission group for him.",
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "Register user",
                "parameters": [
                    {
                        "description": "user id",
                        "name": "user",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/finworker_internal_models_requests_users.RegisterRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "user registered",
                        "schema": {
                            "$ref": "#/definitions/finworker_internal_models_responses_users.RegisterResponse"
                        }
                    },
                    "400": {
                        "description": "test",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/users/{userId}": {
            "get": {
                "description": "get user by user id",
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "Get user",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "user id",
                        "name": "userId",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    },
    "definitions": {
        "finworker_internal_models_requests_users.RegisterRequest": {
            "type": "object",
            "properties": {
                "birthday": {
                    "description": "Birthday. Age must be \u003e 18.",
                    "type": "string"
                },
                "gender": {
                    "description": "Gender. `male`/`female`.",
                    "type": "string"
                },
                "name": {
                    "description": "Name. just to be displayed",
                    "type": "string"
                },
                "password": {
                    "description": "Password, which will be used with secret",
                    "type": "string"
                },
                "username": {
                    "description": "Username, which will be displayed and unique.",
                    "type": "string"
                }
            }
        },
        "finworker_internal_models_responses_users.RegisterResponse": {
            "type": "object",
            "properties": {
                "permission_group": {
                    "description": "PermissionGroup that contains user as owner",
                    "allOf": [
                        {
                            "$ref": "#/definitions/models.PermissionGroup"
                        }
                    ]
                },
                "user": {
                    "description": "User new user data",
                    "allOf": [
                        {
                            "$ref": "#/definitions/models.User"
                        }
                    ]
                },
                "user_permission": {
                    "description": "UserPermission confirms that user is owner of the permission group",
                    "allOf": [
                        {
                            "$ref": "#/definitions/models.UserPermission"
                        }
                    ]
                }
            }
        },
        "models.AccessLevel": {
            "type": "string",
            "enum": [
                "owner",
                "full",
                "read"
            ],
            "x-enum-varnames": [
                "AccessLevelOwner",
                "AccessLevelFull",
                "AccessLevelRead"
            ]
        },
        "models.PermissionGroup": {
            "type": "object",
            "properties": {
                "created_at": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "updated_at": {
                    "type": "string"
                }
            }
        },
        "models.User": {
            "type": "object",
            "properties": {
                "birthday": {
                    "type": "string"
                },
                "gender": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "models.UserPermission": {
            "type": "object",
            "properties": {
                "created_at": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "level": {
                    "$ref": "#/definitions/models.AccessLevel"
                },
                "permission_group_id": {
                    "type": "integer"
                },
                "userId": {
                    "type": "integer"
                }
            }
        }
    }
}